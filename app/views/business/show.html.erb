<div style="margin-left: 0px; margin-right: 0px;">
    <header class='navbar navbar-default navbar-static-top' role='navigation'>
      <div class='row' style="padding-left: 15px">
        <nav class='collapse navbar-collapse'>
          <ul class='nav navbar-nav'>
          	<li class="" style="width: 150px;">
          		<p style="margin-top: 15px; margin-left: 20px;">
          		LS Exchange
          		</p>
          	</li>
            <li class='dropdown'>
              <a href='#' class='dropdown-toggle' data-toggle='dropdown'>
                Dropdown <span class='caret'></span>
              </a>
              <ul class='dropdown-menu'>
                <li><a href='/'>Link</a></li>
                <li class='divider'></li>
                <li><a href='/'>Link</a></li>
              </ul>
            </li>
          </ul>

        </nav>

      </div>
    </header>

    <div style="width: 40%">
		<%
		graph_data = []

		@history.each do |business|

		  date = business.created_at.strftime("%H:%M")

		  data = []
		  data.push(date)
		  data.push(business.value)

		  graph_data.push(data)

		end %>

		<%= line_chart graph_data, legend: false, name: @business.name, data: graph_data, refresh: 10, curve: false, label: "Valor" 
		%>
	</div>
</div>
